Proceso EJERCICIO_7
	Definir num,a,b,c Como real;
	mostrar "VALOR DE LOS GRADOS DEL ÁNGULO: ";
	Leer num;
	a <- sen(num)
	b <- cos(num)
	c <- tan(num)
	mostrar "*RESULTADOS*";
	mostrar "El SENO de: ",num," es ",a;
	mostrar "El COSENO de: ",num," es ",b;
	mostrar "LA TANGENTE de: ",num," es ",c;
FinProceso
